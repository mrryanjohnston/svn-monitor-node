script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js')
script(src='/socket.io/socket.io.js')
script
  var socket = io.connect();
  socket.on('newCommit', function (data) {
    $('#commitlog').prepend('<li><h3>'+data.author+'</h3><h4>'+data.comment+'</h4><p>'data.diff'</p></li>');
  });

#wrapper
  #header
    h1= title
    p Welcome to #{title}
  #leftside
    p sidebar
  #rightside
    p Log:
    ul#commitlog
      !=partial('commit', commits)
  #footer
